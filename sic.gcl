PROCEDURE CSET
PARAMETERS temp field

  IF DEFINED(temp)
    PRINTIN "Setting TEMP = " temp
  ENDIF

  IF DEFINED(field)
    PRINTIN "Setting FIELD = " field
  ENDIF

ENDPROCEDURE

PROCEDURE WAITFOR
PARAMETERS EVENTS
  IF DEFINED(EVENTS)
    PRINTIN "Waiting for " + AS_STRING(EVENTS) + " events."
  ENDIF
ENDPROCEDURE

PROCEDURE BEGIN
PRINTIN "BEGIN RUN"
ENDPROCEDURE

PROCEDURE END
PRINTIN "END RUN"
ENDPROCEDURE
